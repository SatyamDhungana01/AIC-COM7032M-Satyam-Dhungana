<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/geometry_shapes#"
     xml:base="http://www.semanticweb.org/geometry_shapes"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/geometry_shapes"/>

    <!-- Classes -->
    <owl:Class rdf:ID="Shape"/>
    <owl:Class rdf:ID="Triangle">
        <rdfs:subClassOf rdf:resource="#Shape"/>
    </owl:Class>
    <owl:Class rdf:ID="Rectangle">
        <rdfs:subClassOf rdf:resource="#Shape"/>
    </owl:Class>
    <owl:Class rdf:ID="Circle">
        <rdfs:subClassOf rdf:resource="#Shape"/>
    </owl:Class>

    <!-- Data Properties -->
    <owl:DatatypeProperty rdf:ID="Base">
        <rdfs:domain rdf:resource="#Triangle"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </owl:DatatypeProperty>
    <owl:DatatypeProperty rdf:ID="Height">
        <rdfs:domain rdf:resource="#Triangle"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </owl:DatatypeProperty>
    <owl:DatatypeProperty rdf:ID="Length">
        <rdfs:domain rdf:resource="#Rectangle"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </owl:DatatypeProperty>
    <owl:DatatypeProperty rdf:ID="Breadth">
        <rdfs:domain rdf:resource="#Rectangle"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </owl:DatatypeProperty>
    <owl:DatatypeProperty rdf:ID="Radius">
        <rdfs:domain rdf:resource="#Circle"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </owl:DatatypeProperty>
</rdf:RDF>
